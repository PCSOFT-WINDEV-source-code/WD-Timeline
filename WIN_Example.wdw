   -
     name : CreateEvents
     procedure_id : 1230220947357040604
     type_code : 14
     code : |1-
      // Summary: Creates the events that recount the history of Internet
      // Syntax:
      // CreateEvents ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      Procedure CreateEvents()
      
      arrEvents is array of TimelineEvent dynamic
      pclEvent  is TimelineEvent dynamic
      
      pclEvent = new TimelineEvent 
      pclEvent.p_StartDate 		= “19101120”
      pclEvent.p_EndDate   		= “19110527”
      pclEvent.p_Title     		= “Mexican revolution”
      pclEvent.p_Description 	= “The Mexican revolution officially started in Mexico on the 20th of November 1910 and it lasted until the 27th May 1911, when Díaz went in exile. It was followed by a civil war between the revolutionary factions and between the conservator and revolutionary groups.”
      pclEvent.p_ExtendedInfo 	= “http://en.wikipedia.org/wiki/Mexican_Revolution”
      pclEvent.p_Color	 	= LightRed
      Add(arrEvents, pclEvent)
      
      pclEvent = new TimelineEvent 
      pclEvent.p_StartDate 		= “19100822”
      pclEvent.p_EndDate   		= “19100822”
      pclEvent.p_Title     		= “Annexation of Korea by Japan”
      pclEvent.p_Description 	= “Japan officially annexes Korea that is renamed Cho-Sen (end of 1945)”
      pclEvent.p_ExtendedInfo 	= “http://en.wikipedia.org/wiki/Korea_under_Japanese_rule”
      pclEvent.p_Color	 	= LightRed
      Add(arrEvents, pclEvent)
      
      pclEvent = new TimelineEvent 
      pclEvent.p_StartDate 		= “191205142340”
      pclEvent.p_EndDate   		= “191205150220”
      pclEvent.p_Title     		= “Shipwreck of the Titanic”
      pclEvent.p_Description 	= “The shipwreck of the Titanic ended the inaugural journey of the RMS Titanic that was navigating from Southampton to New York. It happens during the night between the 14th and the 15th of April 1912 in the Northern Atlantic ocean off Newfoundland. The ship crashes into an iceberg on the starboard side on the 14th of April 1912 at 23:40 and it sinks in less than three hours, at 02 h 20. Between 1 491 and 1 513 persons died, making this shipwreck one of the biggest maritime catastrophes of all the times.”
      pclEvent.p_ExtendedInfo 	= “http://en.wikipedia.org/wiki/Timeline_of_the_sinking_of_RMS_Titanic”
      pclEvent.p_Color	 	= LightGreen
      Add(arrEvents, pclEvent)
      
      pclEvent = new TimelineEvent 
      pclEvent.p_StartDate 		= “19140804”
      pclEvent.p_EndDate   		= “19181111”
      pclEvent.p_Title     		= “1st World War”
      pclEvent.p_Description 	= “The First World War is a military conflict that happened in Europe from 1914 to 1918. This war represents one of the most important events of the 20th century. It reached a scale and an intensity unknown until then.”
      pclEvent.p_ExtendedInfo 	= “http://en.wikipedia.org/wiki/World_War_I”
      pclEvent.p_Color	 	= LightRed
      Add(arrEvents, pclEvent)
      
      pclEvent = new TimelineEvent 
      pclEvent.p_StartDate 		= “19150401”
      pclEvent.p_EndDate   		= “19160701”
      pclEvent.p_Title     		= “Armenian genocide”
      pclEvent.p_Description 	= “The Armenian genocide happened from April 1915 to July 1916. Most of the Armenian population that lived on the territory of the actual Turkey perished during the internments and the massacres.”
      pclEvent.p_ExtendedInfo 	= “http://en.wikipedia.org/wiki/Armenian_Genocide”
      pclEvent.p_Color	 	= LightYellow
      Add(arrEvents, pclEvent)
      
      pclEvent = new TimelineEvent 
      pclEvent.p_StartDate 		= “19170201”
      pclEvent.p_EndDate   		= “19171001”
      pclEvent.p_Title     		= “Russian revolution”
      pclEvent.p_Description 	= “The Russian revolution represents the events that happened in 1917. In February, the tsarist regime was overthrown and a «Leninist» regime was installed in October.”
      pclEvent.p_ExtendedInfo 	= “http://en.wikipedia.org/wiki/Russian_Revolution_(1917)”
      pclEvent.p_Color	 	= LightRed
      Add(arrEvents, pclEvent)
      
      pclEvent = new TimelineEvent 
      pclEvent.p_StartDate 		= “19291028”
      pclEvent.p_EndDate   		= “19291028”
      pclEvent.p_Title     		= “Crisis of 1929”	
      pclEvent.p_ExtendedInfo 	= “http://en.wikipedia.org/wiki/Great_Depression”
      pclEvent.p_Color	 	= LightGreen
      Add(arrEvents, pclEvent)
      
      pclEvent = new TimelineEvent 
      pclEvent.p_StartDate 		= “19360718”
      pclEvent.p_EndDate  		= “19390401”
      pclEvent.p_Title    		= “Spanish war”
      pclEvent.p_Description 	= “The Spanish war (also called Spanish civil war or Spanish revolution) brought into conflict the «nationalists» with the «republicans» that reunited, sometimes with strong tensions, some communists, socialists, republicans and anarchists.”
      pclEvent.p_ExtendedInfo 	= “http://en.wikipedia.org/wiki/Spanish_Civil_War”
      pclEvent.p_Color	 	= LightRed
      Add(arrEvents, pclEvent)
      
      pclEvent = new TimelineEvent 
      pclEvent.p_StartDate 		= “19370707”
      pclEvent.p_EndDate   		= “19450909”
      pclEvent.p_Title     		= “Second Sino-Japonese war”
      pclEvent.p_Description 	= “The Second Sino-Japonese war was a massive invasion of the easter part of China by the Japanese army.”
      pclEvent.p_ExtendedInfo 	= “http://en.wikipedia.org/wiki/Second_Sino-Japanese_War”
      pclEvent.p_Color	 	= LightRed
      Add(arrEvents, pclEvent)
      
      pclEvent = new TimelineEvent 
      pclEvent.p_StartDate 		= “19330130”
      pclEvent.p_EndDate   		= “19330130”
      pclEvent.p_Title     		= “Hitler comes to power”
      pclEvent.p_Description 	= “Adolf Hitler becomes Chancellor after two meetings with Franz von Papen, leader of a coalition government that implemented a national recovery plan.”
      pclEvent.p_ExtendedInfo 	= “http://en.wikipedia.org/wiki/Adolf_Hitler#Rise_to_power”
      pclEvent.p_Color	 	= LightYellow
      Add(arrEvents, pclEvent)
      
      pclEvent = new TimelineEvent 
      pclEvent.p_StartDate 		= “19390903”
      pclEvent.p_EndDate   		= “19450902”
      pclEvent.p_Title     		= “Second World War”
      pclEvent.p_Description 	= “The Second World War was a worldwide conflict that lasted from September 1939 until September 1945.”
      pclEvent.p_ExtendedInfo 	= “http://en.wikipedia.org/wiki/World_War_II”
      pclEvent.p_Color	 	= LightRed
      Add(arrEvents, pclEvent)
      
      // Assigns the events
      CTPL_TIMELINE_DOUBLE.CTPL_TIMELINE.TimelineControl.p_Events = arrEvents
