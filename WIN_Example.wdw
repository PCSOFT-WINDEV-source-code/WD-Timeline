      // Summary: When a double click is performed on an event
      // Syntax:
      //OnDoubleClick (<clEvent> is TimelineEvent)
      //
      // Parameters:
      //	clEvent (TimelineEvent): Event that was clicked
      // Return value:
      // 	None
      //
      Procedure OnDoubleClick(clEvent is TimelineEvent)
      
      // Starts the corresponding link stored in the event
      ShellExecute(clEvent.p_ExtendedInfo)
